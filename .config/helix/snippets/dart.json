{
  "freezed class": {
    "prefix": "freezed",
    "description": "Freezed class",
    "body": [
      "import 'package:freezed_annotation/freezed_annotation.dart';",
      "",
      "part '${TM_FILENAME_BASE}.freezed.dart';",
      "part '${TM_FILENAME_BASE}.g.dart';",
      "",
      "@freezed",
      "class FooBar with _\\$FooBar {",
      "  factory FooBar.fromJson(Map<String, dynamic> json) => _\\$FooBarFromJson(json);",
      "  const factory FooBar() = _FooBar;",
      "}"
    ]
  },
  "riverpod provider": {
    "prefix": "provider",
    "description": "Riverpod provider",
    "body": [
      "import 'package:riverpod_annotation/riverpod_annotation.dart';",
      "",
      "part '${TM_FILENAME_BASE}.g.dart';",
      "",
      "class FooBarState {}",
      "",
      "@riverpod",
      "FooBarState fooBar(FooBarRef ref) => FooBarState();"
    ]
  },
  "riverpod notifier": {
    "prefix": "notifier",
    "description": "Riverpod notifier",
    "body": [
      "import 'package:riverpod_annotation/riverpod_annotation.dart';",
      "",
      "part '${TM_FILENAME_BASE}.g.dart';",
      "",
      "class FooBarState {}",
      "",
      "@riverpod",
      "class FooBarNotifier extends _\\$FooBarNotifier {",
      "@override",
      "FooBarState build() { return FooBarState(); }",
      "}"
    ]
  },
  "riverpod logic": {
    "prefix": "logic",
    "description": "Logic based on StateNotifier",
    "body": [
      "import 'package:freezed_annotation/freezed_annotation.dart';",
      "import 'package:riverpod_annotation/riverpod_annotation.dart';",
      "",
      "part '${TM_FILENAME_BASE}.freezed.dart';",
      "part '${TM_FILENAME_BASE}.g.dart';",
      "",
      "@freezed",
      "class FooBarState with _\\$FooBarState {",
      "const factory FooBarState() = _FooBarState;",
      "}",
      "",
      "@riverpod",
      "class FooBarLogic extends _\\$FooBarLogic {",
      "@override",
      "FooBarState build() { return FooBarState(); }",
      "}"
    ]
  },
  "chopper service": {
    "prefix": "chopper",
    "description": "Chopper Service",
    "body": [
      "import 'package:chopper/chopper.dart';",
      "",
      "part '${TM_FILENAME_BASE}.chopper.dart';",
      "",
      "@ChopperApi()",
      "abstract class FooBarService extends ChopperService {",
      "  static FooBarService create([ChopperClient? client]) => _\\$FooBarService(client);",
      "}"
    ]
  }
}
